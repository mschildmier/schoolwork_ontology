###  http://www.semanticweb.org/dimmerma/ontologies/2021/1/shoolwork#Schoolwork
:Schoolwork rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty :schoolworkHasInstructor ;
                                                         owl:someValuesFrom :Instructor
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :schoolworkHasStudent ;
                                                         owl:someValuesFrom :Student
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :schoolworkHasSubject ;
                                                         owl:someValuesFrom :Subject
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            owl:disjointWith :Subject .


###  http://www.semanticweb.org/dimmerma/ontologies/2021/1/shoolwork#Student
:Student rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty [ owl:inverseOf :schoolworkHasStudent
                                              ] ;
                               owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                               owl:onClass :Schoolwork
                             ] ;
         rdfs:subClassOf :Role .


###  http://www.semanticweb.org/dimmerma/ontologies/2021/1/shoolwork#Subject
:Subject rdf:type owl:Class .


